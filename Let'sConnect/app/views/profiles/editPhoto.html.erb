


<%= form_for( :profile, url: addProfile_url(@profile), method: :patch , html: { multipart: true , id: current_user.id } ) do |f| %>
	
	<%= f.text_field :user_id, class: 'form-control', value: current_user.id %>
    <%= f.file_field :image, class: "btn btn-primary "  %>
  	<%= f.submit %>
<% end %>
